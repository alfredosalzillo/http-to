{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "import/no-extraneous-dependencies": 0,
      }
    }
  ],
  "plugins": ["jest"]
}
